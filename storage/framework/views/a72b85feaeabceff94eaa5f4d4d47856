            <script>
                $('#myCarousel').carousel({
                    interval: 1000 * 10
                });

                //		$("#locations").onclick = function () {
                //			location.href = "locations.html";
                //		};

            </script>
            <script>
                $('#myCarousel.carousel[data-type="multi"] .item').each(function () {
                    var next = $(this).next();
                    if (!next.length) {
                        next = $(this).siblings(':first');
                    }
                    next.children(':first-child').clone().appendTo($(this));

                    for (var i = 0; i < 4; i++) {
                        next = next.next();
                        if (!next.length) {
                            next = $(this).siblings(':first');
                        }

                        next.children(':first-child').clone().appendTo($(this));
                    }
                });
            </script>
