    <script>

            function initialize() {
                var locations = [];

                <?php if(count($locations)): ?>

                <?php foreach($locations as $index => $location): ?>
                        <?php if((trim($location['long']) != '') && (trim($location['lat'] != ''))): ?>
                        var location = ['<?php echo e($location['name_en']); ?><br/><?php echo e($location['address_en']); ?>', '<?php echo e(trim($location['long'])); ?>', '<?php echo e(trim($location['lat'])); ?>'];
                 console.log(location)
                locations.push(location);
                <?php endif; ?>

                        <?php endforeach; ?>
                        <?php endif; ?>

                        var map = new google.maps.Map(document.getElementById('googleMap'), {
                        zoom: 6,
                                center: new google.maps.LatLng(28.766622, 29.232078),
//                               center: {lat: -34.397, lng: 150.644},
                                mapTypeId: google.maps.MapTypeId.ROADMAP
                        }
                        );

                var infowindow = new google.maps.InfoWindow();

                var marker, i;

                for (i = 0; i < locations.length; i++) {
                    marker = new google.maps.Marker({
                        position: new google.maps.LatLng(locations[i][1], locations[i][2]),
                        map: map
                    });

                    google.maps.event.addListener(marker, 'click', (function (marker, i) {
                        return function () {
                            infowindow.setContent(locations[i][0]);
                            infowindow.open(map, marker);
                        }
                    })(marker, i));
                }

            }
            google.maps.event.addDomListener(window, 'load', initialize);
        </script>
    
    
